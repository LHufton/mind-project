<%- include('../partials/header') %>

<h1>Views Habits Index Page</h1>
<table>
  <thread>
    <tr>
      <th>What did<br>you do?<br></th>
      <th>When did<br>you do it?</th>
      <th>Where did<br>you do it?</th>
      <th>How long<br>did it take?</th>
      <th>Did you <br>finish?</th>
    </tr>
  </thread>
  <tbody>
    <% habits.forEach(function(h) { %>

      <tr>
        <td><%= h.what %></td>
        <td><%= h.when %></td>
        <td><%= h.where %></td>
        <td><%= h.howLong %></td>
        <td><%= h.finished %></td>
        <td><a href="/habits/<%= h.id %>"></a></td>
      </tr> 
    <% }) %>
  </tbody>
</table>
<%- include('../partials/footer') %>
